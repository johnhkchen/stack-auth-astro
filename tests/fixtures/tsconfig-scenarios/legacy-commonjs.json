{
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "lib": ["ES5", "DOM", "ScriptHost"],
    "moduleResolution": "node",
    "strict": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": false,
    "declaration": false,
    "sourceMap": false,
    "downlevelIteration": true,
    "importHelpers": true
  }
}