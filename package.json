{
  "name": "astro-stack-auth",
  "version": "0.1.0",
  "description": "Stack Auth integration for Astro",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "development": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "development": {
        "import": "./dist/server.mjs",
        "require": "./dist/server.cjs"
      },
      "import": "./dist/server.mjs",
      "require": "./dist/server.cjs",
      "default": "./dist/server.mjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "development": {
        "import": "./dist/client.mjs",
        "require": "./dist/client.cjs"
      },
      "import": "./dist/client.mjs",
      "require": "./dist/client.cjs",
      "default": "./dist/client.mjs"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "development": {
        "import": "./dist/components.mjs",
        "require": "./dist/components.cjs"
      },
      "import": "./dist/components.mjs",
      "require": "./dist/components.cjs",
      "default": "./dist/components.mjs"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "development": {
        "import": "./dist/middleware.mjs",
        "require": "./dist/middleware.cjs"
      },
      "import": "./dist/middleware.mjs",
      "require": "./dist/middleware.cjs",
      "default": "./dist/middleware.mjs"
    },
    "./api/handler": {
      "types": "./dist/api/handler.d.ts",
      "development": {
        "import": "./dist/api/handler.mjs",
        "require": "./dist/api/handler.cjs"
      },
      "import": "./dist/api/handler.mjs",
      "require": "./dist/api/handler.cjs",
      "default": "./dist/api/handler.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "build:dev": "NODE_ENV=development tsup",
    "build:analyze": "npm run build && node scripts/build-analyzer.js",
    "build:budget": "npm run build && node scripts/build-analyzer.js",
    "build:budget:strict": "STRICT_BUDGETS=true npm run build:budget",
    "build:budget:dev": "NODE_ENV=development npm run build:dev && NODE_ENV=development node scripts/build-analyzer.js",
    "dev": "NODE_ENV=development tsup --watch",
    "dev:fast": "NODE_ENV=development tsup --watch --no-dts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:perf": "STACK_AUTH_PERF_DEBUG=true vitest",
    "test:perf:watch": "STACK_AUTH_PERF_DEBUG=true vitest --watch",
    "lint": "eslint src tests --ext .ts,.js",
    "lint:fix": "eslint src tests --ext .ts,.js --fix",
    "docs:generate": "node scripts/generate-prop-docs-enhanced.js",
    "docs:extract-types": "node scripts/dynamic-type-extraction.js",
    "docs:detect-changes": "node scripts/interface-change-detector.js",
    "docs:test": "markdown-doctest --config ./.markdown-doctest-setup.cjs spec/features/*.md",
    "docs:test:watch": "markdown-doctest --config ./.markdown-doctest-setup.cjs --watch spec/features/*.md",
    "sprints:test": "markdown-doctest --config ./.markdown-doctest-setup.cjs sprints/**/*.md",
    "test:all": "npm run docs:test && npm run sprints:test",
    "test:components": "vitest run tests/components/",
    "test:components:watch": "vitest tests/components/",
    "test:visual": "playwright test tests/visual/",
    "test:visual:ui": "playwright test tests/visual/ --ui",
    "test:visual:server": "astro dev --port 4321",
    "test:a11y": "playwright test tests/a11y/",
    "test:integration:full": "npm run test:components && npm run test:visual && npm run test:a11y",
    "type:check": "tsc --noEmit",
    "type:check-all": "node scripts/type-check-all-configs.js",
    "type:check-strict": "tsc --noEmit --project test-configs/tsconfig.strict.json",
    "type:check-loose": "tsc --noEmit --project test-configs/tsconfig.loose.json",
    "type:check-tests": "tsc --noEmit --project tsconfig.test.json",
    "type:integration": "npm run type:check-all && npm run build",
    "test:types": "npm run type:integration",
    "test:consumer": "vitest run tests/integration/consumer-validation.test.ts",
    "test:consumer:watch": "vitest --watch tests/integration/consumer-validation.test.ts",
    "test:performance": "STACK_AUTH_PERF_DEBUG=true vitest run && node -e \"require('./tests/utils/performance-regression-detection.js').runPerformanceCheck()\"",
    "test:regression": "node -e \"require('./tests/utils/performance-regression-detection.js').runPerformanceCheck()\"",
    "perf:baseline": "node scripts/performance-baseline-cli.js",
    "perf:baseline:list": "node scripts/performance-baseline-cli.js list",
    "perf:baseline:trends": "node scripts/performance-baseline-cli.js trends",
    "perf:baseline:stats": "node scripts/performance-baseline-cli.js stats",
    "perf:baseline:reset": "node scripts/performance-baseline-cli.js reset --archive",
    "perf:baseline:export": "node scripts/performance-baseline-cli.js export baselines-export.json",
    "perf:baseline:clean": "node scripts/performance-baseline-cli.js clean --days 30",
    "perf:baseline:compare-branches": "node scripts/performance-baseline-cli.js compare-branches",
    "perf:baseline:gradual": "node scripts/performance-baseline-cli.js gradual-regressions",
    "perf:dashboard": "node scripts/performance-dashboard.js generate",
    "perf:dashboard:open": "node scripts/performance-dashboard.js generate --open",
    "build:validate": "node scripts/validate-build.js",
    "build:validate:types": "node scripts/validate-types.js",
    "build:validate:deps": "node scripts/validate-dependencies.js",
    "build:smoke": "node scripts/smoke-test.js",
    "build:test": "npm run build && npm run docs:detect-changes && npm run docs:generate && npm run build:validate && npm run build:validate:types && npm run build:validate:deps && npm run build:smoke && npm run build:budget && npm run test:consumer",
    "build:test:perf": "npm run build:cache:analyze && npm run build:perf:baseline && npm run build:test",
    "validate:components": "node scripts/validate-component-props.js",
    "validate:components:no-fail": "node scripts/validate-component-props.js --no-fail",
    "build:full": "npm run build && npm run validate:components",
    "build:clean": "rm -rf dist",
    "build:cache:clean": "rm -rf node_modules/.tsup",
    "build:cache:analyze": "node scripts/build-cache-analyzer.js",
    "build:cache:analyze:verbose": "node scripts/build-cache-analyzer.js --verbose",
    "build:cache:warm": "node scripts/build-cache-analyzer.js --verbose --no-warm",
    "build:perf": "node scripts/build-performance-monitor.js",
    "build:perf:dev": "node scripts/build-performance-monitor.js --dev --verbose",
    "build:perf:ci": "node scripts/build-performance-monitor.js --ci",
    "build:perf:baseline": "node scripts/build-performance-monitor.js --verbose",
    "build:deps:analyze": "node scripts/dependency-resolution-tracker.js --verbose",
    "build:monitored": "npm run build:cache:analyze && node scripts/build-performance-monitor.js --verbose && npm run build:analyze",
    "build:monitored:dev": "npm run build:cache:analyze && node scripts/build-performance-monitor.js --dev --verbose",
    "build:ci:perf": "node scripts/ci-build-performance.js",
    "build:ci:perf:json": "node scripts/ci-build-performance.js --json",
    "build:ci:perf:strict": "node scripts/ci-build-performance.js --fail-on-regression",
    "build:watch": "npm run dev",
    "prebuild": "npm run build:clean",
    "prepare": "npm run docs:generate",
    "stack:validate": "npm list @stackframe/stack @stackframe/stack-ui",
    "stack:reinstall": "npm uninstall @stackframe/stack @stackframe/stack-ui && npm install @stackframe/stack @stackframe/stack-ui",
    "deps:check": "node scripts/check-optional-deps.js",
    "deps:check:verbose": "node scripts/check-optional-deps.js --verbose",
    "deps:detect": "node scripts/smart-dependency-detector.js",
    "deps:detect:verbose": "STACK_AUTH_DEBUG=true node scripts/smart-dependency-detector.js",
    "test:user-ready": "node scripts/user-testing-validation.js",
    "postinstall": "npm run stack:validate"
  },
  "keywords": [
    "astro",
    "authentication",
    "stack-auth",
    "integration"
  ],
  "author": "Your Name",
  "license": "MIT",
  "peerDependencies": {
    "astro": "^5.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@astrojs/compiler": "^2.10.3",
    "@axe-core/playwright": "^4.8.5",
    "@babel/parser": "^7.26.3",
    "@babel/traverse": "^7.26.4",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/babel__traverse": "^7.20.6",
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^3.2.4",
    "astro": "^5.13.7",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "eslint": "^8.57.1",
    "jsdom": "^26.1.0",
    "markdown-doctest": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@stackframe/stack": "^2.8.36",
    "@stackframe/stack-ui": "^2.8.36"
  }
}
